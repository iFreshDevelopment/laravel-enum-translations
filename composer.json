{
    "version": "1.0.2",
    "license": "MIT",
    "name": "ifresh/laravel-enum-translations",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/translation": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "IFresh\\EnumTranslations\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Kuijer",
            "email": "t.kuijer@ifresh.nl"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "IFresh\\EnumTranslations\\EnumTranslationsServiceProvider"
            ],
            "aliases": {
                "EnumTranslator": "IFresh\\EnumTranslations\\EnumTranslatorFacade"
            }
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.30",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
